class Solution {
public:
    int numberOfEmployeesWhoMetTarget(vector<int>& hours, int target) {
        int r=0;
        int count=0;

        for( int i=0;i<hours.size();i++){
           if (target==hours[i] || hours[i]>target){
                            count++;
                     }
            else{r=0;
                 cout<<r<<endl;
                 }   
        }

        return count; // This ensures that the function always returns a value
    }
};
